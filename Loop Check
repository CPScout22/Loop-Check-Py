for i in range(-20, 40, 5):
    print(i, end = " ")

print()

for i in range(2):
    for j in range(5):
        print("*", end = "")
    print()

num = 50
while num >= 0:
    print(num, end = " ")
    num-=10

word = "yes"

while word != "quit":
    print("Heres your cookie")
    word = input("type yes for a cookie and quit to quit: ")
